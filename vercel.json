{
  "version": 2,
  "builds": [
    {
      "src": "src/web_server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/web_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}